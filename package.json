    "builtin-modules": "^3.3.0",
    "preact": "^10.22.1",
    "rollup": "^3.29.4",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "sass": "^1.77.8",
    "swiper": "^10.3.1",
    "tslib": "^2.6.3",
    "txtgen": "^3.0.7",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.5.3"
